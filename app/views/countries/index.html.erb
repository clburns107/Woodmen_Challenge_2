    });
    */

    $.ajax({
      url: '/countries.json',
      dataType: 'json',
      type: 'get',
      cache: false,
      success: function(data) {
        $(data.countries).each(function(index,value){
          console.log(value.name);

        });
      }
    });
  </script>

<table id="countries_list">
<thead>
